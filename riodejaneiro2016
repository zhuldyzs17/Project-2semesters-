#importing libraries pandas and csv
import csv
import pandas as pd
def calendar_olympic():
    f = pd.read_csv("https://raw.githubusercontent.com/zhuldyzs17/stopwatch-1semesters_project-/main/Calendar.csv")
    print(f)
    

def medals_olympic():
    a = pd.read_csv("https://raw.githubusercontent.com/zhuldyzs17/stopwatch-1semesters_project-/main/Medals.csv")
    print()

def athlets_olympic():
    d = pd.read_csv("https://raw.githubusercontent.com/zhuldyzs17/stopwatch-1semesters_project-/main/Athlets.csv")
    print(d)

def kazakhstan_inRIO():
    x = pd.read_csv("https://raw.githubusercontent.com/zhuldyzs17/stopwatch-1semesters_project-/main/Kazakhstan.csv")
    print(x)

#importing tkinter library
import tkinter as tk

win = tk.Tk()
win.title('Rio de Janeiro 2016')

win.config(bg = '#CCFFFF')
win.geometry("500x400+30+20")
win.minsize(400,300)
win.maxsize(150,150)
win.resizable(True, True)

btn1 = tk.Button(win, text="Calendar",
                 command=calendar_olympic
                 )

btn2 = tk.Button(win, text="Medals",
                 command=medals_olympic
                 )

btn3 = tk.Button(win, text="Athlets",
                 command=athlets_olympic
                 )

btn4 = tk.Button(win, text="Kazakhstan",
                 command=kazakhstan_inRIO
                 )


btn1.pack()
btn2.pack()
btn3.pack()
btn4.pack()


win.mainloop()

